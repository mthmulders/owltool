@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

@prefix foaf: <http://xmlns.com/foaf/0.1/> .

@prefix ex:   <http://purl.org/net/ns/ex#> .

ex:Person
    rdf:type     owl:Class ;
    rdfs:comment "This class serves as input for a basic test case and does not aim to completely describe a Person."@en ;
    rdfs:label   "A person."@en .

ex:hasBirthDate
    rdf:type    owl:ObjectProperty, owl:TransitiveProperty ;
    rdfs:domain ex:Person ;
    rdfs:range  xsd:date .

ex:Patient
    rdf:type        owl:Class ;
    rdfs:subClassOf ex:Person ;
    rdfs:comment    "This class serves as input for a basic test case and does not aim to completely describe a Patient."@en ;
    rdfs:label      "A patient."@en .
